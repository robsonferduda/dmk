{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arcanedev/log-viewer": "4.5.*",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "bensampo/laravel-enum": "^1.16",
        "chumper/zipper": "^1.0",
        "devfactory/minify": "1.0.*",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "hazzard/config": "^1.0",
        "intervention/image": "^2.5",
        "jacobbennett/laravel-http2serverpush": "4.0.2",
        "kodeine/laravel-acl": "~1.0@dev",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^2.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "lavela/phpjasper": "^3.0",
        "maatwebsite/excel": "^3.1",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "mpdf/mpdf": "^8.0",
        "owen-it/laravel-auditing": "8",
        "predis/predis": "^1.1",
        "spatie/laravel-google-calendar": "^2.2",
        "spatie/laravel-sitemap": "5.0.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Hazzard\\Filepicker\\": "app/uploader/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
