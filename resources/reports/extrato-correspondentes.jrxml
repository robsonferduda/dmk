	<queryString>
		<![CDATA[SELECT 
      t2.nm_titular_dba,
      t2.nu_cpf_cnpj_dba,
      t4.nm_banco_ban,  
      t2.nu_agencia_dba,
      t2.nu_conta_dba,
      sum(t5.vl_taxa_honorario_correspondente_pth) as vl_taxa_honorario_correspondente_pth,
      t6.nm_tipo_conta_tcb,
      t0.nm_razao_social_con
FROM
    public.conta_con t0
    INNER JOIN entidade_ete t1 ON (t0.cd_conta_con = t1.cd_conta_con)
    INNER JOIN dados_bancarios_dba t2 ON (t1.cd_entidade_ete = t2.cd_entidade_ete)
    INNER JOIN processo_pro t3 ON (t3.cd_correspondente_cor = t0.cd_conta_con)
    INNER JOIN banco_ban t4 ON (t2.cd_banco_ban = t4.cd_banco_ban)
    INNER JOIN processo_taxa_honorario_pth t5 ON (t3.cd_processo_pro = t5.cd_processo_pro)
    INNER JOIN tipo_conta_banco_tcb t6 ON (t2.cd_tipo_conta_tcb = t6.cd_tipo_conta_tcb)
    INNER JOIN tipo_servico_tse t7 ON (t5.cd_tipo_servico_tse = t7.cd_tipo_servico_tse)
WHERE
    t0.fl_correspondente_con = 'S'
    AND
    t3.deleted_at IS NULL
    $P!{bancoQuery}
GROUP BY 
     t2.nm_titular_dba,
      t2.nu_cpf_cnpj_dba,
      t4.nm_banco_ban,  
      t2.nu_agencia_dba,
      t2.nu_conta_dba,
      t6.nm_tipo_conta_tcb,
      t0.nm_razao_social_con]]>
	</queryString>
